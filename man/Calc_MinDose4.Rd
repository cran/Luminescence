## load example data
data(ExampleData.DeValues)

## apply the logged minimum dose model
Calc_MinDose4(ExampleData.DeValues, 
              sigmab = 0.05, gamma.xub = 10000, mu.xub = 10000, init.p0 = 0.4,
              output.file = FALSE,output.plot = FALSE)
}
